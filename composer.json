{
  "name": "fazed/laravel-strowel",
  "description": "Small package dividing string into blocks using specified delimiters, written for Laravel 5.5 and up.",
  "keywords": ["string", "block", "parser", "laravel", "delimiters", "dividing", "parsing", "delimiting", "analysing", "analyser"],
  "homepage": "https://github.com/fazed/laravel-strowel",
  "license": "MIT",
  "authors": [
    {
      "name": "Fazed",
      "email": "thekoetjeable@gmail.com"
    }
  ],
  "require": {
    "php": "^7.0",
    "illuminate/config": "^5.5|^6.0",
    "illuminate/contracts": "^5.5|^6.0",
    "illuminate/support": "^5.5|^6.0"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master",
    "phpunit/phpunit": "^6.4",
    "orchestra/testbench": "~3.0"
  },
  "autoload": {
    "psr-4": {
      "Fazed\\Strowel\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Fazed\\Strowel\\Test\\": "tests"
    }
  },
  "scripts": {
    "test": "phpunit"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Fazed\\Strowel\\StrowelServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}

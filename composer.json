{
  "name": "fazed/laravel-torrent-title-parser",
  "description": "Small package dividing torrent names into blocks, written for Laravel 5.5 and up.",
  "keywords": ["torrent", "title", "parser", "laravel"],
  "homepage": "https://github.com/fazed",
  "license": "MIT",
  "authors": [
    {
      "name": "Fazed",
      "email": "thekoetjeable@gmail.com"
    }
  ],
  "require": {
    "php": "^7.0",
    "illuminate/config": "^5.5",
    "illuminate/contracts": "^5.5",
    "illuminate/support": "^5.5"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master",
    "phpunit/phpunit": "^6.4",
    "orchestra/testbench": "~3.0"
  },
  "autoload": {
    "psr-4": {
      "Fazed\\TorrentTitleParser\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Fazed\\TorrentTitleParser\\Test\\": "tests"
    }
  },
  "scripts": {
    "test": "phpunit"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Fazed\\TorrentTitleParser\\TorrentTitleParserProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}

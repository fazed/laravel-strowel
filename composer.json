{
  "name": "fazed/laravel-strowel",
  "description": "Small package dividing string into blocks using specified delimiters, written for Laravel 5.5 and up.",
  "keywords": ["string", "block", "parser", "laravel", "delimiters", "dividing", "parsing", "delimiting", "analysing", "analyser"],
  "homepage": "https://github.com/fazed/laravel-strowel",
  "license": "MIT",
  "authors": [
    {
      "name": "Fazed",
      "email": "thekoetjeable@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.2.5",
    "illuminate/config": "^6.0|^7.0",
    "illuminate/contracts": "^6.0|^7.0",
    "illuminate/support": "^6.0|^7.0"
  },
  "require-dev": {
    "dms/phpunit-arraysubset-asserts": "^0.1|^0.2",
    "orchestra/testbench": "^4.0|^5.0",
    "phpunit/phpunit": "^8.0|^9.0"
  },
  "autoload": {
    "psr-4": {
      "Fazed\\Strowel\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Fazed\\Strowel\\Test\\": "tests"
    }
  },
  "scripts": {
    "test": "phpunit"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Fazed\\Strowel\\StrowelServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
